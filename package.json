{
  "name": "vue3-page-transition-monorepo",
  "version": "0.0.1",
  "private": true,
  "description": "A lean Vue.js plugin for page / route transitions.",
  "author": "guygubaby@gmail.com",
  "license": "MIT",
  "keywords": [
    "vue",
    "vuejs3",
    "vue plugin",
    "vue route transition",
    "vue router",
    "vue routing",
    "vue transition",
    "vue animation"
  ],
  "main": "index.js",
  "scripts": {
    "dev": "pnpm -r --parallel --filter !playground run dev",
    "build": "pnpm -r --parallel --filter !playground run build",
    "lint": "eslint \"**/*.{ts,vue,mjs}\"",
    "lint:fix": "pnpm lint --fix",
    "play": "pnpm -C playground run dev",
    "build:play": "pnpm -C playground run build",
    "release": "bumpp package.json packages/*/package.json --commit --push --tag",
    "test": "vitest"
  },
  "dependencies": {
    "prism-theme-vars": "^0.2.4",
    "vue3-page-transition": "workspace:^0.0.1"
  },
  "devDependencies": {
    "@bryce-loskie/eslint-config": "^2.0.4",
    "@iconify-json/carbon": "^1.1.7",
    "@types/markdown-it-link-attributes": "^3.0.1",
    "@types/node": "^18.7.18",
    "@vitejs/plugin-vue": "^3.1.0",
    "@vueuse/core": "^9.2.0",
    "@vueuse/head": "^0.7.10",
    "bumpp": "^8.2.1",
    "critters": "^0.0.16",
    "eslint": "^8.23.1",
    "esno": "^0.16.3",
    "happy-dom": "^6.0.4",
    "jsdom": "^20.0.0",
    "markdown-it-link-attributes": "^4.0.1",
    "markdown-it-prism": "^2.3.0",
    "sass": "^1.54.9",
    "star-markdown-css": "^0.4.1",
    "tsup": "^6.2.3",
    "typescript": "^4.8.3",
    "unocss": "^0.45.21",
    "unplugin-auto-import": "^0.11.2",
    "unplugin-vue-components": "^0.22.7",
    "vite": "^3.1.0",
    "vite-plugin-inspect": "^0.7.1",
    "vite-plugin-md": "^0.20.4",
    "vite-plugin-pages": "^0.26.0",
    "vite-plugin-vue-layouts": "^0.7.0",
    "vite-ssg": "^0.20.2",
    "vite-ssg-sitemap": "^0.4.1",
    "vitest": "^0.23.2",
    "vue": "^3.2.39",
    "vue-router": "^4.1.5"
  }
}
